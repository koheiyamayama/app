<form method="post" action=<%= "/todos/#{@todo.id}"%>>
  <input type="hidden" name="_method" value="patch">
  <label>Title</label>
  <input type="text" name="title" value=<%= @todo.title %>>
  <label>Body</label>
  <input type="text" name="body" value=<%= @todo.body %>>
  <input type="submit">
</form>
